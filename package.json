{
  "name": "serping",
  "version": "1.0.0",
  "description": "Serp.ing SDK",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && npm run build:types && npm run copy",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "clean": "rm -rf dist/*",
    "copy": "cp package.json README.md dist",
    "ts.check": "tsc --project tsconfig.json",
    "release": "npx standard-version --prerelease beta",
    "test": "npx jest"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    }
  },
  "keywords": [
    "serp checker",
    "serp api",
    "serp scraper"
  ],
  "author": "Sead Feng",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "axios-mock-adapter": "^1.22.0",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "pre-commit": [
    "ts.check"
  ]
}
